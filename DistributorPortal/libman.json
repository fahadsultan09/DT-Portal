{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/fontawesome-iconpicker/"
    },
    {
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "Ladda@1.0.6",
      "destination": "wwwroot/lib/Ladda/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/"
    },
    {
      "library": "jquery.blockUI@2.70",
      "destination": "wwwroot/jquery.blockUI/"
    },
    {
      "library": "bootstrap-tagsinput@0.8.0",
      "destination": "wwwroot/lib/plugins/bootstrap-tagsinput/",
      "files": [
        "bootstrap-tagsinput-typeahead.css",
        "bootstrap-tagsinput.css",
        "bootstrap-tagsinput.js",
        "bootstrap-tagsinput.min.js"
      ]
    },
    {
      "library": "microsoft-signalr@5.0.4",
      "destination": "wwwroot/microsoft-signalr/"
    },
    {
      "library": "summernote@0.8.18",
      "destination": "wwwroot/lib/summernote/",
      "files": [
        "summernote-bs4.css",
        "summernote-bs4.js",
        "summernote-bs4.js.map",
        "summernote-bs4.min.css",
        "summernote-bs4.min.js",
        "summernote-bs4.min.js.map",
        "summernote.css",
        "summernote.js",
        "summernote.js.map",
        "summernote.min.css",
        "summernote.min.js",
        "summernote.min.js.map"
      ]
    },
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/jqueryui/",
      "files": [
        "themes/blitzer/jquery-ui.css",
        "themes/blitzer/jquery-ui.min.css",
        "themes/blitzer/theme.css",
        "themes/blitzer/theme.min.css"
      ]
    }
  ]
}